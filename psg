        <script>
            $('.selectors').hover(function(){
                $(this).css('cursor','pointer');
            })
            $('.selectors img').addClass("img-responsive");
            $('.selectors img').css("height","75px");
            $('.selectors').mouseenter(function(){
                $(this).css('transition','0.5s');
                $(this).css('background','black');
            })
            $('.selectors').mouseleave(function(){
                $(this).css('transition','1.5s');
                $(this).css('background','rgba(0, 0, 0, 0.151)');
            })
            $('.selectors').click(function(){
                window.location.href = $(this).attr('redirect');
            })
            $('.redir').click(function(){
                window.location.href = $(this).attr('redir')+"&#lg=1&slide=1";
            })
            $(".navbar-right>li>a").click(function() {
                $('.navbar-right>li').each(function(){
                    if($(this).hasClass("active"))
                    {
                        $(this).removeClass("active");
                    }
                });
                $(this).parent().addClass("active");
                $('html, body').animate({
                    scrollTop: $($(this).attr("scrollDiv")).offset().top -50
                }, 500);
            });
        </script>
        
